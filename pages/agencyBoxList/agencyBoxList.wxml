<!--pages/agency/agency.wxml-->
<view class='them'>
  <view class='list' wx:for='{{packList}}' wx:key="index">
    <view style='display:flex'>
      <view style='width:85px;height:90px;margin-right:10px;background:#f2f2f2'>
        <image lazy-load src="{{item.image}}" style='width:100%;height:100%'></image>
      </view>
    </view>
   <view class='item' style=' text-align: left;'>
      <view style='font-size: 15px;'>{{item.name}}</view>
      <view style='font-size: 15px;'>{{item.specification}}</view>
      <view style='font-size: 15px;'>{{item.id}}</view>
       <view style='font-size: 15px;'>编号{{item.productBarCode}}</view>
    </view>
  </view>

</view>